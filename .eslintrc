{
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "react-app"
  ],
  "parser": "babel-eslint",
  "rules": {
    "camelcase": [
      "error",
      {
        "ignoreDestructuring": true
      }
    ],
    "import/prefer-default-export": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/prop-types": 0,
    "react/state-in-constructor": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js"
        ]
      }
    ]
  }
}